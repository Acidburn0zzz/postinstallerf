[postinstallerf]
name=PostInstallerF
baseurl=http://sourceforge.net/projects/postinstaller/files/fedora/releases/$releasever/$basearch/
enabled=1
metadata_expire=2d
gpgcheck=1
repo_gpgcheck=1
gpgkey=https://raw.githubusercontent.com/kuboosoft/postinstallerf/master/GPG/$releasever/RPM-GPG-KEY-postinstallerf
skip_if_unavailable=1



[postinstallerf-source]
name=PostInstallerF-Source
baseurl=http://sourceforge.net/projects/postinstaller/files/fedora/releases/$releasever/SRPM/
enabled=0
metadata_expire=2d
gpgcheck=1
repo_gpgcheck=1
gpgkey=https://raw.githubusercontent.com/kuboosoft/postinstallerf/master/GPG/$releasever/RPM-GPG-KEY-postinstallerf
skip_if_unavailable=1



[postinstallerf-updates]
name=PostInstallerF-updates
baseurl=http://sourceforge.net/projects/postinstaller/files/fedora/releases/$releasever/$basearch/updates/
enabled=1
metadata_expire=2d
gpgcheck=1
repo_gpgcheck=1
gpgkey=https://raw.githubusercontent.com/kuboosoft/postinstallerf/master/GPG/$releasever/RPM-GPG-KEY-postinstallerf
skip_if_unavailable=1
